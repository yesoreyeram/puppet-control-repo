<%- for @key in @common_settings.keys 
    if !!@common_settings[@key] == @common_settings[@key] -%>
<%= @key %> = <%= @common_settings[@key] ? 'True' : 'False' %>
<%- elsif @common_settings[@key].is_a?(String) -%>
<%= @key %> = '<%= @common_settings[@key] %>'
<%- elsif @common_settings[@key].is_a?(Numeric) -%>
<%= @key %> = <%= @common_settings[@key] %>
<%- elsif @common_settings[@key].is_a?(Array) -%>
<%= @key %> = ['<%= @common_settings[@key].join("','") %>']
<%- elsif @common_settings[@key].is_a?(Hash) -%>
<%= @key %> = {<%- for @nestedkey in @common_settings[@key].keys -%>
    '<%= @nestedkey %>' : {<%- for @doublenestedkey in @common_settings[@key][@nestedkey].keys -%>
            '<%= @doublenestedkey %>' : '<%= @common_settings[@key][@nestedkey][@doublenestedkey] %>',<%- end -%>
    }<%- end -%>
}
<%- end 
    end -%>